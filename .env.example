# .env.example
# Copy to .env for local dev. Never commit your real .env.

# REQUIRED by NWS. Identify your app + contact (email or URL).
NWS_USER_AGENT="go-weather-service/1.0 (you@example.com)"

# Optional thresholds (Fahrenheit)
COLD_LT_F=50# temp < 50 => "cold"
HOT_GE_F=80# temp >= 80 => "hot"

# Server port (your app listens on :8080 regardless; this is here for tools/scripts)
PORT=8080
